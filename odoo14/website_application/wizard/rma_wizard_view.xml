<odoo>
	<data>

		<record id="website_application_inherit_rma_wizard_id" model="ir.ui.view">
            <field name="name">rma.wizard.form.website_application.inherit</field>
            <field name="model">rma.wizard</field>
            <field name="inherit_id" ref="rma.rma_wizard_id"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='qty_return']" position="after">
                	<field name="ticket" />
            	</xpath>
            </field>
        </record>
        <record id="website_application_inherit_rma_form_id" model="ir.ui.view">
            <field name="name">rma.form.website_application.inherit</field>
            <field name="model">rma.rma</field>
            <field name="inherit_id" ref="rma.rma_form_id"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='resolve_date']" position="after">
                	<field name="ticket" />
                    <field name="ticket_iris_link" widget="url"/>
            	</xpath>
            </field>
        </record>
    </data>
</odoo>
