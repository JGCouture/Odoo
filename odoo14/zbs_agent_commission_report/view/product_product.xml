<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record model="ir.ui.view" id="inherit_product_form_agent_commission">
            <field name="name">Product.Inherit.View</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view"/>
            <field name="arch" type="xml">
                <field name="categ_id" position="after">
                    <field name="is_promotion_product"/>
                    <field name="is_package_product"/>
                    <field name="is_shipping_product"/>
                    <field name="is_misc_product"/>
                    <field name="not_show_in_agent_report"/>
                </field>
            </field>
        </record>

    </data>
</odoo>