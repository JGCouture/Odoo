<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="agent_commission_report_wizard" model="ir.ui.view">
            <field name="name">Agent Commission Report</field>
            <field name="model">agent.commission.report</field>
            <field name="arch" type="xml">
                <form string="">
                    <field name="state" invisible="1"/>
                    <field name="name" invisible="1"/>
                    <div states="get">
                       <h2>Report Complete</h2>
                       <p>Here is the Xls File :
                           <field name="data" readonly="1" filename="name"/>
                       </p>
                    </div>
                    <group attrs="{'invisible':[('state','=','get')]}">
                        <field name="employee_id" readonly="1"/>
                        <field name="start_date"/>
                        <field name="end_date"/>
                    </group>
                    <footer>
                        <button attrs="{'invisible':[('state','=','get')]}" string="Print" name="print_xls" class="btn btn-primary" type="object"/>
                        <button attrs="{'invisible':[('state','=','get')]}" string="Cancel" class="btn btn-primary" special="cancel" />
                    </footer>
                </form>
            </field>
         </record>

        <record id="agent_commission_report_wizard_admin" model="ir.ui.view">
            <field name="name">Agent Commission Report</field>
            <field name="model">agent.commission.report</field>
            <field name="inherit_id" ref="zbs_agent_commission_report.agent_commission_report_wizard"/>
            <field name="groups_id" eval="[(4, ref('base.group_system'))]"/>
            <field name="arch" type="xml">
                <field name="employee_id" position="attributes">
                    <attribute name="readonly">0</attribute>
                </field>
            </field>
        </record>

        <record id="action_agent_commission_report" model="ir.actions.act_window">
            <field name="name">Agent Commission Report</field>
            <field name="res_model">agent.commission.report</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

        <menuitem id="menu_agent_commission_report" action="action_agent_commission_report"  parent="sale.sale_menu_root"
                  name="Agent Commission Report"  sequence="600"/>

    </data>
</odoo>